  skos:altLabel "Warmte-index"@nl;
  skos:altLabel "Apparent temperature"@en;
  skos:broader :gevoelstemperatuur;
  dct:source <https://www.knmi.nl/kennis-en-datacentrum/uitleg/gevoelstemperatuur-bij-warm-weer> .

:hittemaatregel a skos:Concept;
  skos:prefLabel "Hittemaatregel"@nl;
