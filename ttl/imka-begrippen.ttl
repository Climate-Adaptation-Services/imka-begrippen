  skos:scopeNote "Hiermee wordt duidelijk waar een lokale adaptatiestrategie op in moet spelen."@nl;
  skos:broader :stresstest;
  rdfs:label "Hittestresstest";
  dct:source <https://klimaatadaptatienederland.nl/stresstest/aanbieders/> .

:hittetoetsingskaart a skos:Concept;
