  skos:broader :temporele_eigenschap;
  rdfs:label "Hittegolffrequentie";
  skos:scopeNote "Als gevolg van klimaatverandering is de frequentie van hittegolven toegenomen."@nl;
  dct:source <https://www.knmi.nl/kennis-en-datacentrum/achtergrond/extreem-weer-en-klimaatverandering>.

:hitte_index a skos:Concept;
  skos:prefLabel "Hitte-index"@nl;
